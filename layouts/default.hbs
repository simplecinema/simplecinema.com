{{#prettify}}
<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
  <meta name="description" content="{{#if description}}{{ description }}{{else}}{{ pkg.description }}{{/if}}" />
  <meta name="keywords" content="{{#if keywords}}{{ keywords }}{{else}}{{join pkg.keywords ','}}{{/if}}" />
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black">
  <link rel="stylesheet" type="text/css" href="{{obj_val compiled_assets "/css/simple-cinema.css"}}">
  <title>{{#if title}}{{ title }}-{{ pkg.NAME }}{{else}}{{ pkg.NAME }}-{{ pkg.keywords }}{{/if}}</title>
</head>

<body{{#if homepage}} class="homepage"{{/if}}>
  <nav class="fixed-top{{#unless homepage}} fixed-visable{{/unless}}" id="navigation">
    <div class="container">
      <div class="row-fluid">
        <div class="span12 center">
          <a class="brand pull-left" href="/">
            <img src="/images/logo.png" alt="Treble">
          </a>
          <div class="mobile-menu" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </div>
          <ul id="main-menu" class="nav-collapse collapse">
            <li><a href="/#page-welcome">首页</a></li>
            <li><a href="/#page-about">关于我们</a></li>
            <li><a href="/#page-work">作品</a></li>
            <li><a href="/#page-contact">联系我们</a></li>
          </ul>
        </div>
      </div>
    </div>
  </nav>

  {{>body}}
  <div class="pages page-contact" id="page-contact">
    <div class="container">
      <header>
        <h4 class="line-divider">是时候探究一下，我们的世界能为您提供什么。</h4>
        <h1>联系我们</h1>
      </header>
      <article>
        <div class="row-fluid">
          <div class="span6 offset1">
            <a href="http://j.map.baidu.com/ukMhy" target="_blank">
              <span class="image-tape bdmap" style="background-image: url('http://api.map.baidu.com/staticimage?center=113.282502,22.805191&amp;width=600&amp;height=600&amp;zoom=17&amp;markers=113.282502,22.805191&amp;markerStyles=l,A,0xff0000');">
              </span>
            </a>
          </div>
          <div class="span5">
            <h5>联系方式</h5>
            <div class="media">
              <div class="social-icons sicon-black pull-left">
                <a class="sicon-phone"><i>电话</i></a>
              </div>
              <div class="media-body smallFontBy08">
                075712345678 / 13888888888
              </div>
            </div>
            <div class="media">
              <div class="social-icons sicon-black pull-left">
                <a class="sicon-place"><i>地址</i></a>
              </div>
              <div class="media-body smallFontBy08">
                地址：广东顺德大良广珠路南一号顺峰山语909-911
              </div>
            </div>
            <div class="media">
              <div class="social-icons sicon-black pull-left">
                <a class="sicon-mail"><i>电邮</i></a>
              </div>
              <div class="media-body smallFontBy08">
                邮箱：info@cstory.cn
              </div>
            </div>
            <div class="media">
              <div class="social-icons sicon-black pull-left">
                <a class="sicon-blank"><i>微博</i></a>
              </div>
              <div class="media-body smallFontBy08">
                微博：<a href="http://weibo.com/cstorys" target="_blank">@cstorys</a>
              </div>
            </div>
            <div class="media">
              <div class="social-icons sicon-black pull-left">
                <a class="sicon-blank"><i>微信</i></a>
              </div>
              <div class="media-body smallFontBy08">
                微信：@纯映文化传播
              </div>
            </div>
            <div class="media">
              <div class="media-body">
                <img src="/images/about/wx-qrcode.jpg" />
              </div>
            </div>
          </div>
        </div>
      </article>
    </div>
  </div>
  <div class="page-foot">
    <div class="container">
      <div class="copy">
        <span>Copyright &copy; 2014 纯映文化传播有限公司 SimpleCinema</span>
      </div>
      <div class="copy hidden-phone">
        <a href="http://www.simplecinema.com/">顺德影视拍摄</a>
        <a href="http://www.simplecinema.com/">企业广告宣传片</a>
        <a href="http://www.simplecinema.com/">顺德广告拍摄</a>
        <a href="http://www.simplecinema.com/">顺德高清影视制作</a>
        <a href="http://cgh.io/" target="_blank">网站制作</a>
        <a href="/sitemap.xml">Sitemap</a>
      </div>
    </div>
  </div>

  <!--[if lt IE 9]>
  <script src="{{obj_val compiled_assets '/js/html5shiv.js'}}"></script>
  <![endif]-->
  <script src="{{obj_val compiled_assets '/js/application.js'}}"></script>
  {{#unless production}}<script src="http://localhost:35729/livereload.js"></script>{{/unless}}
</body>

</html>
{{/prettify}}
